Import('env','easycore','platform')
env_copy = env.Clone()
linreg = env_copy.Program('linreg.cc')
env.Install('#bin/' + platform + '/tools/',linreg)
emmax = env_copy.Program('emmax.cc')
env.Install('#bin/' + platform + '/tools/',emmax)
fastlmm = env_copy.Program('fastlmm.cc')
env.Install('#bin/' + platform + '/tools/',fastlmm)
filter_snps = env_copy.Program('filter_snps.cc')
env.Install('#bin/' + platform + '/tools/',filter_snps)
random_effect_meta_analysis = env_copy.Program('random_effect_meta_analysis.cc')
env.Install('#bin/' + platform + '/tools/',random_effect_meta_analysis)
scones = env_copy.Program('scones.cc')
env.Install('#bin/' + platform + '/tools/',scones)
env_copy.Append(LIBS=['libboost_program_options'])
scones2 = env_copy.Program('scones2.cc')
env.Install('#bin/' + platform + '/tools/',scones2)
